<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hackheroes.lev.achiever">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".activites.mainMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activites.personalActivity" />
        <activity android:name=".activites.settingsActivity" />
        <activity android:name=".activites.statsActivity" />
        <activity android:name=".activites.welcomeNewUserActivity" />
        <activity android:name=".activites.addNewGoalDial" />
        <activity android:name=".activites.todoEdtorActivity"></activity>
    </application>

</manifest>